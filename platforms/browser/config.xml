<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jmdbsoftware.pmaregistrations" version="1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>PMA Conference Registration</name>
    <description>
        Allows users to be registered as PMA members and synced with the main CRM
    </description>
    <author email="james@jmdbsoftware.com" href="https://www.jmdbsoftware.com">
        JMDB Software Ltd
    </author>
    <content src="index.html" />
    <icon height="1024" src="www/res/icon/ios/PMA-1024x1024.png" width="1024" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="phonegap-version" value="cli-9.0.0" />
</widget>
